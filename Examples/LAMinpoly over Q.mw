</Group><CodeEditor-ExecGroup view="presentation" labelreference="L171" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion1" expanded="false" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">#Monomlist 
#input: a polynomial f, a list of monomials called B, the list of variables called X
#Output:Coordinate vector of f w.r.t B
with(LinearAlgebra):
Monomlist:=proc(f1,B1,X)
local m,v,i,mon,j,cof,c,d,f,B;
#At some examples B1 is Vector and the nops(B1) does not give the correct result so we need to convert it to list
   f:=expand(rpoly(f1));
   B:=convert(B1,list);
   d:=nops(B);
   v:=Vector(d); 
   c:=coeffs(f,X,'m');
   cof:=[c];
   mon:=[m];
   for j to nops(mon) do
      if member(mon[j],B,'i') then
         v[i]:=cof[j];
      else error &quot;monomial not in B&quot;
      fi;
   od;
return v;
end:
#RED
#input: a polynomial, a, the list of minimal polynomials,m, and the list of variables,X.
#output: the polynomial ,a, mod m[1],m[2],...
RED:=proc(m,p)
   local  M,i;
      M:=[seq(0,i=1..nops(m))];
      #Reduce polynomial a mod p
      #a1:=phirpoly(a,p);
      for i  to nops(m) do
         #Reduce minimal polynomials mod p
         M[i]:=phirpoly(m[i],p);
      od;
       #Reduce a1 mod minimal polynomials
      return(M);
   end:
 #Basemaker
#input:The list of variables, the list of degree of each variable
#output: The list of all the possible monomials constructed by X mod m[i]s
Basemaker := proc(X,D::list(integer))
local x,i,M,m,Z;
if nops(X)=1 then
   x := X[1];
   seq( x^i,i=0..D[1]-1 );
else
   M := Basemaker(X[2..-1],D[2..-1]);
   x := X[1];
   Z := [seq( x^i,i=0..D[1]-1 )];
   [seq(seq( x*m, m in M ), x in Z)]
fi
end: 
LAMinpoly:=proc(m,p,gama,X,Z)
   local n,deg,d,monombasis,g,A,sol,i,j,b,inp,gammaminpoly,B,gg,M;
     n:=nops(m); #nops(m)=nops(X)
     deg:=[seq(degrpoly(m[i],X[-i]),i=1..n)];
     d:=mul(deg[i],i=1..n);
     monombasis:=Basemaker(X,deg); 
     #It is a basis for K=Z_p[z1,z2]/&lt;z1^2-2,z2^2-3&gt;: {1,z1,z2,z1z2}
     #construct gamma=x1+c_1*x2+...+c_n-1*xn
     #gama:=X[1]+add(C[i]*X[i+1],i=1..n-1); 
     #Finding the powers of gama mod m[i]s
      #g[0]:=1;
      M:=RED(m,p); #Reduce minimal polynomials mod p
      #Construct the ring to which gamma belongs
      g[1]:=phirpoly(rpoly(gama,X),p,seq(M)); 
      g[0] := rpoly(1,getring(g[1]));
     for i from 2 to d do
        g[i]:= mulrpoly(g[1],g[i-1]);
     od;
     A:=Matrix(d);
     #Creating the matrix of coeffs
     for j from 1 to d do
         A[1..d,j]:=Monomlist(g[j-1],monombasis,X);
     od;    #Check for appropriate C here:If det(A)&lt;&gt;0, then  A is invertible and we are good. Otherwise, MGCD goes back and chooses another C   
     if Det(A) mod p=0 then return(FAIL); fi;
     b:=-Monomlist(g[d],monombasis,X);
     #sol:=LinearSolve(A,b); #As det(A)&lt;&gt;0 we are sure that there is a solution
     #Constructing the polynomial
     B:=A^(-1) mod p;
     sol:=B.b;
     gammaminpoly:=add(sol[i]*Z^(i-1),i=1...d)+Z^d mod p;  
    return gammaminpoly, A,B;
end:
